{                                                                              }
{==============================================================================}
{                                                                              }
{  Parameters             Passed By            Description                     }
{  ----------             ---------            -----------                     }
{                                                                              }
{==============================================================================}

ROUTINE get_word_res_array

DECLARE i
i = 1

end_of_array = SIZE_OF_ARRAY ( res_arr )

word_res_array [ i , 1 ] = "TEST"
word_res_array [ i , 2 ] = "SPECIFICATIONS"
word_res_array [ i , 3 ] = "RESULT"

WHILE ( i < end_of_array + 1 )

    i = i + 1

    word_res_array [ i , 1 ] = STRIP ( res_arr [ i - 1 , TEST_NAME ] )
    word_res_array [ i , 3 ] = STRIP ( res_arr [ i - 1 , RES_VAL   ] )
    word_res_array [ i , 2 ] = STRIP ( res_arr [ i - 1 , SPC_VALUE ] )

ENDWHILE

ENDROUTINE


