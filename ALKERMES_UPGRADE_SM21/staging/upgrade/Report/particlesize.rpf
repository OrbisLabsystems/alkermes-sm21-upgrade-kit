{ ROUTINE output_gui_report                     sends report data to a         }
{                                               specified template in MS Word  }
{                                                                              }
{==============================================================================}
{                                                                              }
{  Modification History                                                        }
{  --------------------                                                        }
{                                                                              }
{  Date         By              Description                                    }
{  ----         --              -----------                                    }
{  24/4/96      JMWHYTE         Created                                        }
{  20/10/97     JOD             Modifications for LRC                          }
{                                                                              }
{==============================================================================}
{                                                                              }
{  Parameters             Passed By            Description                     }
{  ----------             ---------            -----------                     }
{                                                                              }
{==============================================================================}


ROUTINE output_gui_report

    word . connect ()

    status = word . file_open ( LOGO_DIR:"SAR_LRC.DOT" )
    IF  status <> EMPTY THEN
        flash_message ( LOGO_DIR:"SAR_LRC Template cannot be found, " :
                        "status = ":status, TRUE )
        EXIT
    ENDIF

    IF ( word . End_of_document () <> EMPTY ) THEN EXIT ENDIF

    get_form_data ()


    word . send ( "StartOfDocument")
    word . send ( "EndOfDocument 1")
    word . send ( "EditClear")

    word . left_paragraph ()

    word . insert_paragraph ()
    font . points = 12
    word . set_font ( font )

    word . insert_array ( line_1_array , 1, 2, 0 , FALSE )
    word . insert_array ( line_2_array , 1, 2, 0 , FALSE )
    word . insert_paragraph ()

    word . insert_array ( word_res_array, i, 3, 16 , FALSE )
    word . insert_paragraph ()
    word . insert_paragraph ()

    font . bold = TRUE
    word . set_font ( font )

    word . insert_array ( line_3_array , 1, 1, 0 , TRUE )
    word . insert_paragraph ()
    word . insert_paragraph ()

    word . insert_array ( line_4_array , 1, 2, 0 , TRUE )

    word . send ( "UpdateFields" )
    word . send ( "Beep" )

    word . disconnect ()


ENDROUTINE

