ROUTINE output_char_cell_report

SET NAME "DISPLAY/"
j = 1

LITERAL
SampID Analysis   Component                   Result           Spec. Value
------ ---------- --------------------------- ---------------- -----------------
$ENDLITERAL
       WHILE res_arr [ j , TEST_NAME ] <> EMPTY
           aa = res_arr [ j , TEST_NAME ]
           bb = res_arr [ j , RES_VAL   ]
           cc = res_arr [ j , TEST_UNIT ]
           dd = res_arr [ j , TEST_STAT ]
           ee = res_arr [ j , SMP_NAME  ]
           ff = res_arr [ j , SMP_STAT  ]
           gg = res_arr [ j , ANL_NAME  ]
           hh = res_arr [ j , SPC_VALUE ]
LITERAL
$ee___ $gg_______ $aa________________________ $bb_____________ $hh______________
$ENDLITERAL
        j = j + 1
   ENDWHILE

FLUSH_LITERAL

ENDROUTINE


{==============================================================================}
{                                                                              }
{ ROUTINE get_form_data             Gets data that is to be output to the      }
{                                   report                                     }
{                                                                              }
{==============================================================================}
{                                                                              }
{  Modification History                                                        }
{  --------------------                                                        }
{                                                                              }
{  Date         By              Description                                    }
{  ----         --              -----------                                    }
{  24/4/96      JMWHYTE         Created                                        }
{  20/10/97     JOD             Modifications for LRC                          }
{                                                                              }
{==============================================================================}

ROUTINE get_form_data

    product_id = SELECT SAMPLE . product_name
    prod_ver   = SELECT SAMPLE . product_version
    prod_ver   = STRIP ( product_id ) : "/ver." : STRIP ( prod_ver )

    IF (product_id <> EMPTY) AND (product_id <> "") THEN {MR, added product_id <>""}
        prod_desc = SELECT MLP_HEADER . description
                    WHERE identity = product_id
    ELSE
        flash_message ( "No Material Description Found", TRUE )
        prod_desc = ""
    ENDIF

    lot_number = SELECT SAMPLE . batch_name
    IF lot_number = EMPTY THEN lot_number = "" ELSE
        lot_number = STRIP ( lot_number )
    ENDIF

    lrc_by = OPERATOR ()
    get_person ( lrc_by )

    auth_notes = STRIP ( SELECT SAMPLE . authorisation_notes )

    line_1_array [ 1 , 1 ] = "Lot Number : "      : lot_number
    line_1_array [ 1 , 2 ] = "Product :  "        : prod_desc
    line_2_array [ 1 , 1 ] = "Part Code : "       : product_id
