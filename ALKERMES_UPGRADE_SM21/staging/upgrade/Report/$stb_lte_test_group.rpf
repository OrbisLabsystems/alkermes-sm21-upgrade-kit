{*******************************************************************************
*
* Module Name : $STB_LTE_TEST_GROUP
*
* Purpose : LTE Support for Stability Test Group
*           Implements the APPROVE and HISTORY Functionality for
*           Stability Test Group 
*
* Document Ref. : N/a
*
*******************************************************************************}

ENABLE WINDOWS
SET NAME "DEFER/"
SET COMPILE_OPTION DECLARE
SET NOTPROTECTED

JOIN LIBRARY $LIB_LTE_APPROVE
JOIN LIBRARY $LIB_UTILS
JOIN LIBRARY $GEN_APPR

{*******************************************************************************
*                            M A I N C O D E
*******************************************************************************}

DECLARE option
option = 1234
history_study_test_group (Option)
EXIT

{*******************************************************************************
*                            R O U T I N E S
*******************************************************************************}

ROUTINE approve_study_test_group ( VALUE option )

{ 
* Approve Study Test Group
*
* Parameters   : option - Menu Procedure Number
* Return Value : N/a
*
*******************************************************************************}

    approve ( "STB_TEST_GROUP" )

ENDROUTINE

{******************************************************************************}

ROUTINE history_study_test_group ( VALUE option )

{ 
* Study Test Group Approval History
*
* Parameters   : option - Menu Procedure Number
* Return Value : N/a
*
*******************************************************************************}

    inspection_labtable_history ( "STB_TEST_GROUP", "" )

ENDROUTINE

{******************************************************************************}
